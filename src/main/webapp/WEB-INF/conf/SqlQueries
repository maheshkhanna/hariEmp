create database Hari;

CREATE TABLE USERS(ID int auto_increment,USER_NAME VARCHAR(100),PASSWORD VARCHAR(100),primary key(id));

CREATE TABLE OPERATION_DETAILS(RECORD_ID BIGINT auto_increment,USER_ID int,FIRST_OFF VARCHAR(200),PRODUCT_LANE VARCHAR(200),PART_NUMBER VARCHAR(200),STATUS VARCHAR(200),TIME_IN BIGINT,TIME_OUT BIGINT,DURATION BIGINT, FOREIGN KEY(USER_ID) REFERENCES USERS(ID),primary key(RECORD_ID));

#To add a new user
INSERT INTO USERS (USER_NAME,PASSWORD) VALUES ("mahesh","khanna");

INSERT INTO OPERATION_DETAILS (USER_ID,FIRST_OFF,PRODUCT_LANE,PART_NUMBER,STATUS,TIME_IN,TIME_OUT,DURATION) VALUES (1,"Op-30","10 Speed","2351","Accepted",1613752005020,1613752017216,12196);